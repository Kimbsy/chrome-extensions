{
	"manifest_version": 2,
	"name" : "ScrollOverUnder",
	"description": "displays XKCD webcomic scrollover text under the comic",

	"version": "1.0",

	"content_scripts": [
		{
		"matches": ["http://xkcd.com/*", "http://what-if.xkcd.com/*"],
		"js": ["js/jquery-2.1.1.js", "js/scrollOverUnder.js"]
		}
	]
}